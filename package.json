{
  "name": "iobroker.megabas",
  "version": "0.0.1",
  "description": "Adapter to use the Building Automation Stackable Card for Raspberry Pi from Sequent Technologies",
  "author": {
    "name": "Florian Grubert",
    "email": "privat@fgrubert.de"
  },
  "homepage": "https://github.com/Florian Grubert/ioBroker.megabas",
  "license": "MIT",
  "keywords": [
    "megabas",
    "megabas-rpi",
    "sequent technologies",
    "building automation",
    "raspberry pi"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Florian Grubert/ioBroker.megabas"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@iobroker/adapter-react": "^1.4.8",
    "@iobroker/testing": "^2.3.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/gulp": "^4.0.7",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.8",
    "@types/proxyquire": "^1.3.28",
    "@types/react-dom": "^16.9.9",
    "@types/react": "^16.9.56",
    "@types/sinon": "^9.0.8",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "axios": "^0.21.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "gulp": "^4.0.2",
    "mocha": "^8.2.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "proxyquire": "^2.1.3",
    "react-dom": "^16.14.0",
    "react-icons": "^3.11.0",
    "react": "^16.14.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "main": "build/main.js",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:parcel": "parcel build admin/src/index.tsx -d admin/build",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:ts && npm run build:parcel",
    "watch:parcel": "parcel admin/src/index.tsx -d admin/build",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/"
  },
  "bugs": {
    "url": "https://github.com/Florian Grubert/ioBroker.megabas/issues"
  },
  "readmeFilename": "README.md"
}
